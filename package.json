{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bin": {
    "repo-check": "dist/cli.js"
  },
  "bugs": "https://github.com/Shuunen/repo-checker/issues",
  "config": {
    "logFile": "repo-check.log"
  },
  "description": "Repo cleaning made easy",
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@types/require-from-string": "1.2.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "arg": "5.0.0",
    "ava": "3.14.0",
    "c8": "7.3.5",
    "colorette": "1.2.1",
    "eslint": "7.16.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-unicorn": "24.0.0",
    "esm": "3.2.25",
    "npm-run-all": "4.1.5",
    "require-from-string": "2.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.35.1",
    "rollup-plugin-add-shebang": "0.3.1",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-typescript2": "0.29.0",
    "shuutils": "4.1.0",
    "ts-node": "9.1.1",
    "tslib": "2.0.3",
    "typescript": "4.1.3"
  },
  "files": [
    "dist",
    "templates"
  ],
  "homepage": "https://github.com/Shuunen/repo-checker",
  "keywords": [
    "repository",
    "check",
    "checker"
  ],
  "license": "GPL-3.0",
  "main": "dist/cli.js",
  "name": "repo-check",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/repo-checker.git"
  },
  "scripts": {
    "build": "rollup -c",
    "check": "node .",
    "ci": "npm run lint && npm run check && npm run test",
    "dev": "nodemon --quiet --ignore dist/ --exec npm run check",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.ts .",
    "postversion": "git push && git push --tags && npm publish",
    "prebuild": "rimraf dist",
    "precheck": "npm run build",
    "prepare": "npm run build",
    "preversion": "npm run ci",
    "start": "npm run check",
    "test": "c8 --check-coverage --lines 90 --functions 90 --branches 90 --include 'src/*.ts' ava",
    "test:watch": "nodemon --quiet --exec npm test --ext .ts",
    "update": "npx npm-check-updates -u"
  },
  "type": "module",
  "version": "1.14.2"
}
