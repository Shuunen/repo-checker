{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bin": {
    "repo-check": "src/index.js"
  },
  "bugs": "https://github.com/Shuunen/repo-checker/issues",
  "config": {
    "logFile": "repo-check.log"
  },
  "description": "Repo cleaning made easy",
  "devDependencies": {
    "arg": "4.1.0",
    "chalk": "2.4.2",
    "cross-var": "1.1.0",
    "eslint": "6.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "simple-git": "1.116.0"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/Shuunen/repo-checker",
  "keywords": [
    "repository",
    "check",
    "checker"
  ],
  "license": "GPL-3.0",
  "main": "src/index.js",
  "name": "repo-check",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/repo-checker.git"
  },
  "scripts": {
    "dev": "cross-var nodemon $npm_package_main --target=.. --fix",
    "lint": "eslint --fix src/**/*.js",
    "start": "cross-var node $npm_package_main",
    "test": "npm run lint && cross-var node $npm_package_main --target=.",
    "update": "npx npm-check-updates -u",
    "postversion": "git push && git push --tags && npm publish"
  },
  "version": "1.1.0"
}
