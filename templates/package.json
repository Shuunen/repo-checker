{
  "author": "{{ user_name }} <{{ user_mail }}>",
  "bugs": "https://github.com/{{ user_id }}/{{ repo_id }}/issues",
  "description": "",
  "devDependencies": {
    "repo-check": "latest"
  },
  "files": [
    "src",
    "*"
  ],
  "homepage": "https://github.com/{{ user_id }}/{{ repo_id }}",
  "keywords": [],
  "license": "{{ license }}",
  "name": "@{{ user_id_lowercase }}/{{ repo_id }}",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/{{ user_id }}/{{ repo_id }}.git"
  },
  "scripts": {
    "check": "repo-check",
    "ci": "npm run test",
    "dev": "npm start",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.html .",
    "postinstall": "npm run check",
    "postversion": "git push && git push --tags",
    "preversion": "npm run ci",
    "start": "",
    "test": "npm run lint && npm run check",
    "update": "npx npm-check-updates -u"
  },
  "version": "0.0.1"
